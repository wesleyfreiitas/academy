    <!-- Seção "Sobre" -->
    <section id="about" class="py-5">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <h2>Sobre Nós</h2>
                    <p>Conheça mais sobre nossa plataforma omnichannel e os treinamentos que oferecemos.</p>
                </div>
                <div class="col-lg-6">
                    <img src="about-image.jpg" alt="Sobre Nós" class="img-fluid">
                </div>
            </div>
        </div>
    </section>

    <!-- Seção "Vídeos" -->
    <section id="videos" class="py-5">
        <div class="container">
            <h2>Vídeos de Treinamento</h2>
            <div class="row">
                <!-- Adicione vídeos aqui -->
                <div class="col-lg-4">
                    <iframe width="100%" height="200" src="https://www.youtube.com/embed/XkftkE2SaNY"
